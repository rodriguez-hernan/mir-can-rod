{
  "name": "zen-garden",
  "version": "1.0.0",
  "description": "Zen Garden project for class WDD 331",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel src/index.html -d build/",
    "prebuild": "rimraf dist/*",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html -d build/",
    "lint": "eslint *.js src/**/*.js",
    "sass": "sass src/scss/style-guide.scss src/styles.css"
  },
  "author": "Hernan Rodriguez",
  "license": "",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0"
  },
  "dependencies": {}
}
